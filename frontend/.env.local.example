# Get a free project ID from https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Replace with your deployed contract addresses (after running Deploy.s.sol on Base Sepolia)
NEXT_PUBLIC_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DAO_ADDRESS=0x0000000000000000000000000000000000000000

# Supabase PostgreSQL (auto-set by Vercel Supabase integration)
# Transaction pooler (port 6543) for app runtime
POSTGRES_PRISMA_URL=postgres://postgres.xxxx:password@aws-0-us-west-2.pooler.supabase.com:6543/postgres?pgbouncer=true
# Direct connection (port 5432) for Prisma migrations
POSTGRES_URL_NON_POOLING=postgres://postgres.xxxx:password@aws-0-us-west-2.pooler.supabase.com:5432/postgres

# NextAuth / Auth.js secret -- generate with: openssl rand -base64 32
AUTH_SECRET=your_secret_here
NEXTAUTH_SECRET=your_secret_here

# App URL (set automatically by Vercel, only needed locally)
NEXTAUTH_URL=http://localhost:3000

# (Optional) Resend API key for email notifications
RESEND_API_KEY=
NOTIFY_EMAIL=
